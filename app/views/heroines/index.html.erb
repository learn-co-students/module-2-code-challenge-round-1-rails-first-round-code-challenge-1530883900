<h2>All Heroines</h2>

<br>
<%= form_tag heroines_path, method: :get, id: "search-form" do %>
  <%= text_field_tag :search, params[:search], placeholder: "Search by Supoerpowers"%>
  <%= submit_tag 'Search', :name => nil %>
<% end %>
<br>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>
