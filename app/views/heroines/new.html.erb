<h1>Create a Heroine: </h1>

<%= form_for @heroine do |f| %>
<%= f.label :name %>:<br>
<%= f.text_field :name %><br></br>
<%= f.label :super_name %><br>
<%= f.text_field :super_name %><br></br>
<%= f.label :super_power %><br>
<%= f.collection_select :power_id, Power.all, :id, :name, prompt: 'Choose a super power'%><br></br>
<%= f.submit %>
<% end %>
