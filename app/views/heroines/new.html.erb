<h1>Create A Heroine!</h1>
<hr><hr>
<%= button_to "All Heroines", heroines_path, method: "get" %>
<hr>

<%= form_for @heroine do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <br>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %><br>
  <br>
  <%= f.collection_select :power_id, Power.all, :id, :name %><br>
  <br>
  <%= f.submit %>
<% end %>
