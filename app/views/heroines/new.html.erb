<h1>Create a Heroine</h1>

<%= form_for @heroine do |f| %>
<%= f.label :name %>:
<%= f.text_field :name %> <br> <br>

<%= f.label :super_name %>:
<%= f.text_field :super_name %> <br> <br>

<%= f.label :power_id, :Power %>:
<%= collection_select(:heroine, :power_id, Power.all, :id, :name) %>
<br>
<br>
<%= f.submit %>

<% end %>
